# Knowledge RAG Web UI - Environment Configuration
# Copy this file to .env and update with your actual values

# API Endpoints (adjust ports as needed)
VITE_API_URL=http://localhost:3001
VITE_RAG_URL=http://localhost:8002
VITE_KG_URL=http://localhost:8001
VITE_VECTOR_URL=http://localhost:8003
VITE_UNIFIED_URL=http://localhost:8004

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:8005

# Application Configuration
VITE_APP_NAME="Knowledge RAG Web UI"
VITE_APP_VERSION="0.1.0"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_E2E_TESTS=false

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info

# Optional: External Services
# VITE_SENTRY_DSN=your_sentry_dsn_here
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Note: Never commit the actual .env file to version control
# This file serves as a template for environment configuration